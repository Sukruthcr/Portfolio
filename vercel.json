{
  "builds": [
    {
      "src": "app/frontend/package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "build" }
    },
    {
      "src": "app/backend/server.py",
      "use": "@vercel/python"
    }
  ],
  "rewrites": [{ "source": "/api/(.*)", "destination": "/app/backend/server.py" }]
}